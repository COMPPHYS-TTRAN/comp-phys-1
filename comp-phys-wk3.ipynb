    "#Numerical Instability and arbitrary Precision\n",
    "import decimal\n",
    "decimal.getcontext().prec = 30\n",
    "D = decimal.Decimal\n",
    "\n",
    "def g(t):\n",
    "    return t**(-6)\n",
    "\n",
    "def diff2(f, x, h=1e-9):\n",
    "    x = D(str(x)); h = D(str(h))\n",
    "    r = (f(x-h) - 2*f(x) + f(x+h)) / (h*h)\n",
    "    return r\n",
    "\n",
    "for k in range(1, 15):\n",
    "    h = 10**(-k)\n",
