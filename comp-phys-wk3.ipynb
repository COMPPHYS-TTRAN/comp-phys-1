    "def g(t):\n",
    "    return t**(-6)\n",
    "\n",
    "def diff2(f, x, h=1e-6):\n",
    "    r = (f(x-h) - 2*f(x) + f(x+h))/float(h*h)\n",
    "    return r\n",
    "\n",
    "for k in range(1, 15):\n",
    "    h = 10**(-k)\n",
    "    d2g = diff2(g, 1,h)\n",
    "    print d2g"
